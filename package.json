{
  "name": "vue-accounts-manager-simple",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently -n \"VUE,API\" \"npm run dev\" \"npm run api\"",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint . --fix",
    "lint:eslint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/",
    "api": "json-server --watch db.json --port 3000 --host 127.0.0.1",
    "api:cors": "json-server --watch db.json --port 3000 --host 127.0.0.1 --middlewares ./middleware.js"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.13.4",
    "pinia": "^3.0.4",
    "vue": "^3.5.27",
    "vue-router": "^5.0.1",
    "vuetify": "^3.11.8"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.10.12",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "~1.42.0",
    "eslint-plugin-vue": "~10.7.0",
    "jiti": "^2.6.1",
    "json-server": "^1.0.0-beta.5",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.42.0",
    "prettier": "3.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.2.4"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
